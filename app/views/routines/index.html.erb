<p id="notice"><%= notice %></p>

<h1>Routines</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Desc</th>
      <th>Start date</th>
      <th>End date</th>
      <th>Routine type</th>
      <th>Days</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @routines.each do |routine| %>
      <tr>
        <td><%= routine.title %></td>
        <td><%= routine.desc %></td>
        <td><%= routine.start_date %></td>
        <td><%= routine.end_date %></td>
        <td><%= routine.routine_type_id %></td>
        <td><%= routine.days %></td>
        <td><%= link_to 'Show', routine %></td>
        <td><%= link_to 'Edit', edit_routine_path(routine) %></td>
        <td><%= link_to 'Destroy', routine, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Routine', new_routine_path %>
